#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules/@babel/cli/bin/node_modules:/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules/@babel/cli/node_modules:/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules/@babel/node_modules:/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules:/mnt/c/Users/Gökhan/Desktop/node_modules:/mnt/c/Users/Gökhan/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules/@babel/cli/bin/node_modules:/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules/@babel/cli/node_modules:/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules/@babel/node_modules:/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules:/mnt/c/Users/Gökhan/Desktop/node_modules:/mnt/c/Users/Gökhan/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Gökhan/Desktop/my-codes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/cli/bin/babel.js" "$@"
else
  exec node  "$basedir/../@babel/cli/bin/babel.js" "$@"
fi
